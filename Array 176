#include <stdio.h>
int main() 
{
    int n, search, found = 0;
    scanf("%d", &n);
    int arr[n];
    for (int i = 0; i < n; i++)
    {
    scanf("%d", &arr[i]);
    }
    scanf("%d", &search);
    for (int i = 0; i < n; i++) 
    {
    if (arr[i] == search) 
    {
    printf("Door Number is %03d-DN\n", i); 
    found = 1;
    break;
    }
    }
    if (!found)
    {
    printf("-1\n");
    }
    return 0;
}
