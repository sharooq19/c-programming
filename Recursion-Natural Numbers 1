#include <stdio.h>
int main() 
{
    int n;
    scanf("%d", &n);
    if (n < 1 || n > 20) 
    {
    printf("Enter a Valid Input!!!!!!!!!!!!!!!!!!!!!!!!\n");
    }  
    else 
    {
    printf("The first %d Natural Numbers are:\n", n);
    for (int i = 1; i <= n; i++)
    {
    printf("%d ", i);
    }
    printf("\n");
    }
    return 0;
}
