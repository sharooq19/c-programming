#include <stdio.h>
int main() 
{
    int num;
    scanf("%d", &num);
    if (num >= 100 && num <= 999) 
    {
    int sum = 0;
    sum += num / 100;
    sum += (num / 10) % 10;
    sum += num % 10;
    printf("Sum of Digit:%04d\n", sum);
    } 
    else 
    {
    printf("Enter a Valid Input!!!!!!!\n");
    }
    return 0;
}
